  <Paragraph>
    <Number>1</Number>
    <StartMilliseconds>0</StartMilliseconds>
    <EndMilliseconds>4164</EndMilliseconds>
    <Text>[g@Golden Horn scowled at Wukong.</Text>
  </Paragraph>
  <Paragraph>
    <Number>2</Number>
    <StartMilliseconds>4165</StartMilliseconds>
    <EndMilliseconds>8061</EndMilliseconds>
    <Text>"I doubt you have a gourd like mine."@g]</Text>
  </Paragraph>
  <Paragraph>
    <Number>3</Number>
    <StartMilliseconds>8062</StartMilliseconds>
    <EndMilliseconds>10982</EndMilliseconds>
