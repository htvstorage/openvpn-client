<script type="text/javascript">

var PLAYER_CDN_HOST = "//cdn.littlefox.com";
var ROOT_PATH = "/en";
var VIEW_MODE = "view";
var POP_TITLE = "Little Fox";

var isSafari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent);

$(document).ready(function() {
    if (Get_Cookie('already_fox_player_open') == 'Y') {
        alert('There is already a player currently in use.');
        self.close();
    } else {
        setCookie('already_fox_player_open', 'Y', 2);

        interval = setInterval(function () {
            setCookie('already_fox_player_open', 'Y', 2);
        }, 1000);

        $(window).on('beforeunload', function () {
            clearInterval(interval);
            setCookie('already_fox_player_open', 'Y', -1);
        });
    }

        if ( isSafari ) {
                $("#video").prop('muted', true);
                $(".btn_sound_on").hide();
                $(".btn_sound_off").show();
                $(".sound_layer").addClass('show');
                setTimeout(function(){
                        $(".sound_layer").removeClass('show');
                },10000);
        }


        var video_data = new Array();


        if (typeof contents_info[0].fc_id == 'undefined' ) {
                alert('컨텐츠 정보가 없습니다.');
                self.close();
        }
        $.each(contents_info, function(idx, d) {console.log(d);

                subtitle_url = d.type == 'S' ? "https://cdn.littlefox.co.kr/contents/caption/"+d.fc_id+".xml?v="+d.mod_date : "";
                video_data.push({
                        fc_id: d.fc_id,
                        type: d.type, // 동화 S , 동요 M
                        charge: d.charge, // 유료 유무 ( Y : 유료 / F : 무료 )
                        play_time: parseFloat(d.play_time),
                        prev_time: parseFloat(d.prev_time), // 미리보기 시간 (초)
                        content_level: d.content_level, // 컨텐츠 레벨
                        video_url: d.video_url, // video URL
                        subtitle: subtitle_url, // 자막 URL
                        title_time: parseFloat(d.title_time), // 타이틀 보여지는 시간 (ms)
                        next_url: d.next_fc_id, // 다음회 URL
                        player_link: {
                                eBook: d.ebook,
                                quiz: d.quiz,
                flashcard: d.flashcards,
                starWords: d.starwords,
                crossword: d.crossword,
                                recorder: d.recorder,
                                mp3Download: d.mp3download,
                                printAbleBook: d.printablebook,
                                text: d.text,
                                writing_topics: d.writing_topics,
                                flash_card: d.flash_card,
                                with_mom: d.with_mom,
                                topic: d.topic,
                                tracing: 'N',
                                worksheet: d.worksheet,
                paint: d.paint
                        },
                        // fg_content_yn: d.fg_content_yn,
                        cont_title: d.cont_title,
                        fs_id: d.fs_id,
                        play_subtime: parseFloat(d.play_subtime)
                });
        });

        if ( video_data.length > 0 )
        {
                player_h5({
                        video: video_data,
                        login_yn: (Cookie.get('fx0') && Cookie.get('fx5') == 'UA') || "view"=='admin_view' ? true:false, // login 유무
                        relay: 0 || (video_data.length > 1 ? true : false)
                });
        }

        // 처음 오셨나요?
        $('.first-go-btn').on('click',function(e){
                e.preventDefault();
        window.open("/"+LANG+'/introduce', 'introduce');
        });

        $('.free-go-btn').on('click',function(e){
                e.preventDefault();
                opener.parent.location="/"+LANG+'/free';
                window.close();
        });

    $('.payment-go-btn').on('click',function(e){
        e.preventDefault();

        if (Cookie.get('fx7')) {
            opener.parent.location = '/'+LANG+'/'+(LANG == 'ko' ? 'user' : 'subscribe')+'/payment';
        }  else {
            if (LANG == 'ko') {
                opener.parent.location = '/'+LANG+'/help/fee';
            } else {
                opener.parent.location = '//user.littlefox.com/'+LANG+'/subscribe/join';
            }
        }

        window.close();
    });

    //재생 속도 컨트롤창 이외의 부분 클릭시 컨트롤 창 숨김
    $('body').on('click', function(e){
        var tgPoint = $(e.target);

        if($('#main-choose-box').css('display') == "block" || $('#play-choose-box').css('display') == "block")
        {
            if(tgPoint.attr('name') != "video-radio" && tgPoint.attr('name') != "intro-radio" && tgPoint.attr('name') != "playback-sel")
            {
                $('.choose-box').hide();
            }
        }
    });

    //코칭창 열기
    if(Cookie.get('player_coaching_anymore') == null || Cookie.get('player_coaching_anymore') != 'Y')
    {
        //동화인 경우에만
        if(contents_info[0]['type'] == 'S'){
            $('.player-coaching').addClass('on');
        }
    }

        // 컨트롤바 숨기기
        if($('.player-coaching').hasClass('on')){
                $('.player_control').css('opacity','0');
        }

        // 코칭마크 닫기 및 컨트롤바 띄우기
        $('.player-coaching button').on('click',function(){
                $('.player-coaching').removeClass('on');
                $('.player_control').css('opacity','1');

                // 더 이상 보지 않기 쿠키 처리
                if($('input:checkbox[id="chk-anymore"]').is(":checked"))
        {
            Cookie.set('player_coaching_anymore', 'Y', 365);
        }
        });

        // 재생속도 조절 레이어 on/off
        $('.btn_speed .icon-box').on('click',function(){
                $(this).siblings('.choose-box').toggle();
        });
});
</script>
