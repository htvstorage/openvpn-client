        window.close();
    });

    //재생 속도 컨트롤창 이외의 부분 클릭시 컨트롤 창 숨김
    $('body').on('click', function(e){
        var tgPoint = $(e.target);

        if($('#main-choose-box').css('display') == "block" || $('#play-choose-box').css('display') == "block")
        {
            if(tgPoint.attr('name') != "video-radio" && tgPoint.attr('name') != "intro-radio" && tgPoint.attr('name') != "playback-sel")
            {
                $('.choose-box').hide();
            }
        }
    });

    //코칭창 열기
    if(Cookie.get('player_coaching_anymore') == null || Cookie.get('player_coaching_anymore') != 'Y')
    {
        //동화인 경우에만
        if(contents_info[0]['type'] == 'S'){
            $('.player-coaching').addClass('on');
        }
    }

	// 컨트롤바 숨기기
	if($('.player-coaching').hasClass('on')){
		$('.player_control').css('opacity','0');
	}

	// 코칭마크 닫기 및 컨트롤바 띄우기
	$('.player-coaching button').on('click',function(){
		$('.player-coaching').removeClass('on');
		$('.player_control').css('opacity','1');

		// 더 이상 보지 않기 쿠키 처리
		if($('input:checkbox[id="chk-anymore"]').is(":checked"))
        {
            Cookie.set('player_coaching_anymore', 'Y', 365);
        }
	});

	// 재생속도 조절 레이어 on/off
	$('.btn_speed .icon-box').on('click',function(){
		$(this).siblings('.choose-box').toggle();
	});
});
</script>
